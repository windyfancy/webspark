<div class="sidebar">
    <div>栏目</div>
    <ul class="card">
        <% var rows=catalogList;
        for(var i=0;i<rows.length;i++){%>
            <li catalogId="<%=rows[i].id%>">
                <a href="<%='/catalog?id='+rows[i].id%>"><%=rows[i].title%>(<%=rows[i].count%>)</a></h2>
            </li>
        <%}%>
    </ul>
    <div>标签</div>
    <ul class="card">
        <% var rows=tagList;
        for(var i=0;i<rows.length;i++){%>
            <li catalogId="<%=rows[i].id%>">
                <a href="<%='/tag?id='+rows[i].id%>"><%=rows[i].title%>(<%=rows[i].count%>)</a></h2>
            </li>
        <%}%>
    </ul>
    <div>常用链接</div>
    <ul>
            <% var rows=linkList.friend;
            for(var i=0;i<rows.length;i++){%>
             <li>
                <a href="<%=rows[i].linkUrl%>"><%=rows[i].title%></a></h2>
             </li>
            <%}%>
    </ul>
</div>